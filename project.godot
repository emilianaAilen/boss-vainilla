; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "BaseScene",
"language": "GDScript",
"path": "res://scenes/SceneBase.gd"
}, {
"base": "Control",
"class": "InteractuableAbstract",
"language": "GDScript",
"path": "res://scenes/InteractuableBase.gd"
}, {
"base": "Control",
"class": "SceneRoomBase",
"language": "GDScript",
"path": "res://scenes/SceneRoomBase.gd"
} ]
_global_script_class_icons={
"BaseScene": "",
"InteractuableAbstract": "",
"SceneRoomBase": ""
}

[application]

config/name="twisted"
run/main_scene="res://scenes/Main.tscn"
config/icon="res://icon.png"

[autoload]

GameState="*res://globals/game_state.gd"
AudioManager="*res://scenes/AudioManager.tscn"
ScenesManager="*res://scenes/ScenesManager.tscn"

[display]

window/stretch/aspect="keep_width"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
